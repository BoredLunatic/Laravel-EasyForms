{
    "name": "second2none/easy-forms", 
    "description": "Generate, validate and process Vueitfy forms in laravel. Build frontend and backend all from laravel.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kane Grills",
            "email": "kane@s2npro.site"
        }
    ],
    "minimum-stability": "dev-master",
    "autoload": {
        "psr-4": {
            "Second2None\\EasyForms\\App\\": "src/" ,
            "Second2None\\EasyForms\\Database\\Migrations\\": "database/migrations/" ,
            "Second2None\\EasyForms\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "require": {
        "nesbot/carbon": "^2.16"
    },
    "require-dev": {
        "laravel/ui": "^3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Second2None\\EasyForms\\App\\Providers\\EasyForms"
            ],
            "aliases": {
                "EasyForms": "Second2None\\EasyForms\\App"
            }
        }
    }
}
